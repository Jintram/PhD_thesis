file = {:C$\backslash$:/Users/wehrens/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Caporaso et al. - 2011 - Moving pictures of the human microbiome.pdf:pdf},
isbn = {1465-6914 (Electronic)$\backslash$n1465-6906 (Linking)},
issn = {1465-6914},
journal = {Genome Biology},
keywords = {16S,16S: genetics,Bacteria,Bacteria: classification,Bacteria: genetics,Computer Simulation,Gastrointestinal Tract,Gastrointestinal Tract: microbiology,Genomics,Genomics: methods,High-Throughput Nucleotide Sequencing,Humans,Inflammatory Bowel Diseases,Inflammatory Bowel Diseases: microbiology,Metagenome,Metagenome: genetics,Obesity,Obesity: microbiology,Organ Specificity,RNA,Ribosomal,Seasons,Time Factors,reviewpnas2016},
