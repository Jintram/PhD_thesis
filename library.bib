journal = {Genetics},
keywords = {Alleles,Bacteriophage lambda,Bacteriophage lambda: genetics,Base Pair Mismatch,Base Pair Mismatch: genetics,Chromosome Segregation,Chromosome Segregation: genetics,DNA, Bacterial,DNA, Bacterial: genetics,DNA, Single-Stranded,DNA, Single-Stranded: genetics,Escherichia coli,Escherichia coli: virology,Genetic Engineering,Genetic Engineering: methods,Mutation,Mutation: genetics,Phosphorothioate Oligonucleotides,Phosphorothioate Oligonucleotides: genetics,Recombination, Genetic},
