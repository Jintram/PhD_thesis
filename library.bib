journal = {PloS one},
keywords = {Animals,Caenorhabditis elegans,Caenorhabditis elegans Proteins,Caenorhabditis elegans Proteins: metabolism,Caenorhabditis elegans: enzymology,Caenorhabditis elegans: genetics,Caenorhabditis elegans: metabolism,Chromatin,Chromatin: metabolism,Dosage Compensation, Genetic,Dosage Compensation, Genetic: genetics,Histone-Lysine N-Methyltransferase,Histone-Lysine N-Methyltransferase: deficiency,Histones,Histones: chemistry,Histones: genetics,Histones: metabolism,Lysine,Male,Methylation,Nuclear Proteins,Nuclear Proteins: metabolism,X Chromosome,X Chromosome: genetics,X Chromosome: metabolism},
month = {jan},
