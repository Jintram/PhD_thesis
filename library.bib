file = {:C$\backslash$:/Users/wehrens/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/English et al. - 2011 - Single-molecule investigations of the stringent response machinery in living bacterial cells.pdf:pdf},
