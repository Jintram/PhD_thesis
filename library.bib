issn = {1551-8507},
journal = {WormBook : the online review of C. elegans biology},
keywords = {Animals,Caenorhabditis elegans,Caenorhabditis elegans: genetics,Chromosome Segregation,Disorders of Sex Development,Dosage Compensation, Genetic,Female,Gene Expression Regulation,Male,Sex Determination Processes,Signal Transduction,X Chromosome},
