file = {:C$\backslash$:/Users/wehrens/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Model, Burkhardt - 2001 - A standard for calibration and shading correction of a fluorescence microscope.pdf:pdf},
