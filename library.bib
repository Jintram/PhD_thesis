ts within a cytoplasmic meshwork.},
author = {Konopka, Michael C and Shkel, Irina a and Cayley, Scott and Record, M Thomas and Weisshaar, James C},
doi = {10.1128/JB.01982-05},
file = {:C$\backslash$:/Users/wehrens/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Konopka et al. - 2006 - Crowding and confinement effects on protein diffusion in vivo.pdf:pdf},
issn = {0021-9193},
journal = {Journal of bacteriology},
keywords = {Biological Transport,Buffers,Culture Media,Diffusion,Escherichia coli,Escherichia coli: growth {\&} development,Escherichia coli: metabolism,Green Fluorescent Proteins,Green Fluorescent Proteins: metabolism,Osmosis},
month = {sep},
