keywords = {Animals,Biological,Extracellular Signal-Regulated MAP Kinases,Extracellular Signal-Regulated MAP Kinases: geneti,Extracellular Signal-Regulated MAP Kinases: metabo,Extracellular Signal-Regulated MAP Kinases: physio,Humans,Models,Neoplasms,Neoplasms: genetics,Neoplasms: therapy,Signal Transduction,Signal Transduction: genetics,Signal Transduction: physiology,ras Proteins,ras Proteins: genetics,ras Proteins: metabolism,ras Proteins: physiology},
month = {may},
