issn = {1936-122X},
journal = {Annual review of biophysics},
keywords = {min oscillations,pattern formation},
pages = {315--336},
pmid = {21545286},
title = {{Protein self-organization: lessons from the min system.}},
volume = {40},
year = {2011}
}
