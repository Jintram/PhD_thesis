keywords = {Computational Biology,Computer Simulation,Feedback,Fungal,Gene Expression Regulation,Gene Regulatory Networks,Genetic,Physiological,Physiological: genetics,Repressor Proteins,Saccharomyces cerevisiae,Saccharomyces cerevisiae: genetics,Tetracycline,Transcription,noiseRobust,qbio2016,toRead},
