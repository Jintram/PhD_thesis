issn = {1478-3975},
journal = {Physical biology},
keywords = {Environment,Escherichia coli,Escherichia coli: genetics,Escherichia coli: metabolism,Gene Expression Regulation,Genetic,Models,Phenotype,Stochastic Processes,Time Factors,qbio2016},
mendeley-tags = {qbio2016},
number = {4},
