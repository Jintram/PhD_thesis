issn = {1097-4172},
journal = {Cell},
pages = {1--14},
pmid = {26478179},
title = {{Global Brain Dynamics Embed the Motor Command Sequence of Caenorhabditis elegans}},
url = {http://dx.doi.org/10.1016/j.cell.2015.09.034},
volume = {163},
