journal = {Molecular systems biology},
keywords = {Epigenesis, Genetic,Escherichia coli,Escherichia coli: genetics,Escherichia coli: growth {\&} development,Lac Operon,Microfluidic Analytical Techniques,Microscopy, Fluorescence,Stochastic Processes,Systems Biology,Systems Biology: methods},
