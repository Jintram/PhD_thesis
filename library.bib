journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Animals,Binding Sites,Crystallography, X-Ray,Humans,Mice,Models, Molecular,Multiprotein Complexes,Protein Conformation,Protein Structure, Tertiary,Recombinant Proteins,Recombinant Proteins: chemistry,Recombinant Proteins: metabolism,SOS1 Protein,SOS1 Protein: chemistry,SOS1 Protein: metabolism,ras-GRF1,ras-GRF1: chemistry,ras-GRF1: metabolism},
