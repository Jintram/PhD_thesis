issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
month = {jul},
number = {31},
pmid = {25056965},
title = {{Single-particle tracking reveals that free ribosomal subunits are not excluded from the Escherichia coli nucleoid.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25056965},
volume = {111},
year = {2014}
}
