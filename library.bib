volume = {110},
year = {2013}
}
@article{Stephens2011,
abstract = {Sister chromatid cohesion provides the mechanistic basis, together with spindle microtubules, for generating tension between bioriented chromosomes in metaphase. Pericentric chromatin forms an intramolecular loop that protrudes bidirectionally from the sister chromatid axis. The centromere lies on the surface of the chromosome at the apex of each loop. The cohesin and condensin structural maintenance of chromosomes (SMC) protein complexes are concentrated within the pericentric chromatin, but whether they contribute to tension-generating mechanisms is not known. To understand how pericentric chromatin is packaged and resists tension, we map the position of cohesin (SMC3), condensin (SMC4), and pericentric LacO arrays within the spindle. Condensin lies proximal to the spindle axis and is responsible for axial compaction of pericentric chromatin. Cohesin is radially displaced from the spindle axis and confines pericentric chromatin. Pericentric cohesin and condensin contribute to spindle length regulation and dynamics in metaphase. Together with the intramolecular centromere loop, these SMC complexes constitute a molecular spring that balances spindle microtubule force in metaphase.},
