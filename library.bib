file = {:C$\backslash$:/Users/wehrens/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Abriata, Spiga, Dal Peraro - 2013 - All-atom simulations of crowding effects on ubiquitin dynamics.pdf:pdf},
