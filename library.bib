keywords = {Bacteria,Bacteria: cytology,Bacteria: genetics,Bacteria: growth {\&} development,Cell Respiration,Gene Expression Regulation,Macrophages,Macrophages: microbiology,Microbiological Processes,Microfluidic Analytical Techniques,Microscopy, Fluorescence,Stochastic Processes},
