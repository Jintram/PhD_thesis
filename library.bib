journal = {Nature protocols},
keywords = {Escherichia coli,Escherichia coli: genetics,Gene Expression Regulation, Bacterial,Microscopy, Fluorescence,Microscopy, Fluorescence: methods,Single-Cell Analysis,Time-Lapse Imaging,Time-Lapse Imaging: methods},
month = {jan},
number = {1},
pages = {80--8},
pmid = {22179594},
title = {{Measuring single-cell gene expression dynamics in bacteria using fluorescence time-lapse microscopy.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22179594},
volume = {7},
year = {2012}
