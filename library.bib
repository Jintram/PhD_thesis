keywords = {Bayes Theorem,Computer Simulation,Escherichia coli,Escherichia coli: genetics,Gene Expression Regulation,Gene Expression Regulation, Bacterial,Gene Regulatory Networks,Lac Operon,Lac Operon: genetics,Transcription, Genetic},
month = {apr},
number = {17},
pages = {7151--6},
pmid = {17420464},
title = {{Noisy information processing through transcriptional regulation.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1855426{\&}tool=pmcentrez{\&}rendertype=abstract},
