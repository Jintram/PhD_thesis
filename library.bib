file = {:C$\backslash$:/Users/wehrens/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Plowman et al. - 2005 - H-ras, K-ras, and inner plasma membrane raft proteins operate in nanoclusters with differential dependence on th.pdf:pdf},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Actins,Actins: physiology,Animals,Cells, Cultured,Cricetinae,Cytoskeleton,Cytoskeleton: physiology,Membrane Microdomains,Membrane Microdomains: chemistry,Membrane Proteins,Membrane Proteins: chemistry,Models, Theoretical,ras Proteins,ras Proteins: chemistry},
