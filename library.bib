@article{Ruhe2015,
abstract = {CdiB/CdiA proteins mediate inter-bacterial competition in a process$\backslash$ntermed contact-dependent growth inhibition (CDI). Filamentous CdiA$\backslash$nexoproteins extend from CDI+ cells and bind specific receptors to$\backslash$ndeliver toxins into susceptible target bacteria. CDI has also been$\backslash$nimplicated in auto-aggregation and biofilm formation in several species,$\backslash$nbut the contribution of CdiA-receptor interactions to these$\backslash$nmulticellular behaviors has not been examined. Using Escherichia coli$\backslash$nisolate EC93 as a model, we show that cdiA and bamA receptor mutants are$\backslash$ndefective in biofilm formation, suggesting a prominent role for$\backslash$nCdiA-BamA mediated cell-cell adhesion. However, CdiA also promotes$\backslash$nauto-aggregation in a BamA-independent manner, indicating that the$\backslash$nexoprotein possesses an additional adhesin activity. Cells must express$\backslash$nCdiA in order to participate in BamA-independent aggregates, suggesting$\backslash$nthat adhesion could be mediated by homotypic CdiA-CdiA interactions. The$\backslash$nBamA-dependent and BamA-independent interaction domains map to distinct$\backslash$nregions within the CdiA filament. Thus, CdiA orchestrates a collective$\backslash$nbehavior that is independent of its growth-inhibition activity. This$\backslash$nadhesion should enable `greenbeard' discrimination, in which genetically$\backslash$nunrelated individuals cooperate with one another based on a single$\backslash$nshared trait. This kind-selective social behavior could provide$\backslash$nimmediate fitness benefits to bacteria that acquire the systems through$\backslash$nhorizontal gene transfer.},
