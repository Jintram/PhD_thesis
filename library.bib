file = {:C$\backslash$:/Users/wehrens/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kremling et al. - 2014 - Understanding carbon catabolite repression in Escherichia coli using quantitative models.pdf:pdf},
