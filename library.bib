keywords = {Algorithms,Bacterial Physiological Phenomena,Environment,Epigenesis, Genetic,Escherichia coli,Escherichia coli: metabolism,Genes, Bacterial,Models, Biological,Models, Genetic,Models, Theoretical,Mutation,Phenotype,Stochastic Processes,Time Factors},
month = {apr},
