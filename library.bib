file = {:C$\backslash$:/Users/wehrens/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Pogliano, Beckwith - 1994 - SecD and SecF facilitate protein export in Escherichia coli.pdf:pdf},
