file = {:C$\backslash$:/Users/wehrens/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Dave et al. - 2012 - The human gut microbiome Current knowledge, challenges, and future directions.pdf:pdf},
isbn = {1878-1810 (Electronic)$\backslash$r1878-1810 (Linking)},
issn = {19315244},
journal = {Translational Research},
keywords = {reviewPNAS2016},
mendeley-tags = {reviewPNAS2016},
number = {4},
