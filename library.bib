pages = {137--51},
pmid = {16204833},
title = {{Stochastic simulation of chemical reactions with spatial resolution and single molecule detail.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16204833},
volume = {1},
year = {2004}
}
