issn = {1478-3975},
journal = {Physical biology},
month = {aug},
number = {4},
pages = {045002},
pmid = {23912905},
title = {{The effect of crowding and confinement: a comparison of Yfh1 stability in different environments.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23912905},
volume = {10},
year = {2013}
}
@article{Martins2011,
abstract = {Sensing extracellular changes initiates signal transduction and is the first stage of cellular decision-making. Yet relatively little is known about why one form of sensing biochemistry has been selected over another. To gain insight into this question, we studied the sensing characteristics of one of the biochemically simplest of sensors: the allosteric transcription factor. Such proteins, common in microbes, directly transduce the detection of a sensed molecule to changes in gene regulation. Using the Monod-Wyman-Changeux model, we determined six sensing characteristics--the dynamic range, the Hill number, the intrinsic noise, the information transfer capacity, the static gain, and the mean response time--as a function of the biochemical parameters of individual sensors and of the number of sensors. We found that specifying one characteristic strongly constrains others. For example, a high dynamic range implies a high Hill number and a high capacity, and vice versa. Perhaps surprisingly, these constraints are so strong that most of the space of characteristics is inaccessible given biophysically plausible ranges of parameter values. Within our approximations, we can calculate the probability distribution of the numbers of input molecules that maximizes information transfer and show that a population of one hundred allosteric transcription factors can in principle distinguish between more than four bands of input concentrations. Our results imply that allosteric sensors are unlikely to have been selected for high performance in one sensing characteristic but for a compromise in the performance of many.},
author = {Martins, Bruno M C and Swain, Peter S},
doi = {10.1371/journal.pcbi.1002261},
file = {:C$\backslash$:/Users/wehrens/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Martins, Swain - 2011 - Trade-offs and constraints in allosteric sensing.pdf:pdf},
issn = {1553-7358},
journal = {PLoS computational biology},
keywords = {Allosteric Regulation,Bacterial Physiological Phenomena,Gene Expression Regulation,Models, Biological,Signal Transduction,Systems Biology,Systems Biology: methods,Transcription Factors,Transcription Factors: physiology},
month = {nov},
number = {11},
pages = {e1002261},
pmid = {22096453},
title = {{Trade-offs and constraints in allosteric sensing.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3207937{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {7},
year = {2011}
}
