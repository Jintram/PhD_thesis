keywords = {cell cycle,cyclin d1,farnesyltransferase inhibitor,nuclear localization,ras},
month = {feb},
number = {2},
pages = {166--72},
pmid = {21779490},
title = {{H-ras localizes to cell nuclei and varies with the cell cycle.}},
