keywords = {Antigens, CD59,Antigens, CD59: chemistry,Cell Membrane,Cell Membrane: metabolism,Computer Simulation,Diffusion,Membrane Proteins,Membrane Proteins: chemistry,Models, Chemical,Models, Statistical,Reproducibility of Results,Signal Transduction,Stochastic Processes,ras Proteins,ras Proteins: chemistry},
month = {apr},
