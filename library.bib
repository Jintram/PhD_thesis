pmid = {25174572},
title = {{The proton-motive force is required for translocation of CDI toxins across the inner membrane of target bacteria}},
volume = {94},
year = {2014}
}
@article{Hasselmann1988,
abstract = {A general method is described for constructing simple dynamical models to approximate complex dynamical systems with many$\backslash$n                     degrees of freedom. The technique can be applied to interpret sets of observed time series or numerical simulations with high-resolution$\backslash$n                     models, or to relate observation and simulations. The method is based on a projection of the complete system on to a smaller$\backslash$n                     number of âprincipal interaction patternsâ (PIPs). The coefficients of the PIP expansion are assumed to be governed by a dynamic$\backslash$n                     model containing a small number of adjustable parameters. The optimization of the dynamical model, which in the general case$\backslash$n                     can be both nonlinear and time-dependent, is carried out simultaneously with the construction of the optimal set of interaction$\backslash$n                     patterns. In the linear case the PIPs reduce to the eigenoscilations of a first-order linear vector process with stochastic$\backslash$n                     forcing (principal oscillation patterns, or POPs). POPs are linearly related to the âprincipal prediction patternsâ used in$\backslash$n                     linear forecasting applications. The POP analysis can also be applied as a diagnostic tool to compress the extensive information$\backslash$n                     contained in the high-dimensional cross-spectral covariance matrix representing the complete second-moment structure of the$\backslash$n                     system.},
author = {Hasselmann, K.},
