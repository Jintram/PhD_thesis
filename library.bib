title = {{Indirect effects of ploidy suggest X chromosome dose, not the X:A ratio, signals sex in Drosophila.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2222971{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {5},
