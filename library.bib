keywords = {Algorithms,Bacterial Proteins,Bacterial Proteins: chemistry,Calibration,Cell Division,Cell Division: physiology,Escherichia coli,Escherichia coli: physiology,Fluorescence,Luminescent Proteins,Luminescent Proteins: chemistry,Microscopy, Fluorescence},
month = {jul},
