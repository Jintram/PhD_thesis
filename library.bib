keywords = {Amino Acids,Amino Acids: metabolism,Biosynthetic Pathways,Biosynthetic Pathways: genetics,Fungal,Gene Expression Profiling,Gene Expression Regulation,Gene Regulatory Networks,Gene Regulatory Networks: genetics,Genetic,Metabolic Networks and Pathways,Metabolic Networks and Pathways: genetics,Models,Nucleotides,Nucleotides: biosynthesis,Saccharomyces cerevisiae,Saccharomyces cerevisiae: genetics,printed,toread},
