journal = {Genes {\&} development},
keywords = {Adenosine Triphosphatases,Adenosine Triphosphatases: metabolism,Animals,Caenorhabditis elegans,Caenorhabditis elegans Proteins,Caenorhabditis elegans Proteins: metabolism,Caenorhabditis elegans: genetics,Caenorhabditis elegans: metabolism,DNA-Binding Proteins,DNA-Binding Proteins: metabolism,Dosage Compensation,Female,Gene Expression Regulation,Genetic,Genetic: genetics,Genome,Helminth,Helminth: genetics,Male,Multiprotein Complexes,Multiprotein Complexes: metabolism,Mutation,Nuclear Proteins,Protein Binding,Protein Subunits,Protein Subunits: metabolism,X Chromosome,X Chromosome: genetics,X Chromosome: metabolism},
