issn = {01681605},
journal = {International Journal of Food Microbiology},
keywords = {Filamentation,Food safety,Morphology,Reduced energy state,Risk assessment,Sublethal stress},
number = {2},
pages = {97--110},
pmid = {23727653},
publisher = {Elsevier B.V.},
title = {{Filament formation by foodborne bacteria under sublethal stress}},
url = {http://dx.doi.org/10.1016/j.ijfoodmicro.2013.05.001},
volume = {165},
year = {2013}
}
