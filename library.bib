journal = {PLoS biology},
keywords = {Animals,Drosophila Proteins,Drosophila Proteins: genetics,Drosophila Proteins: metabolism,Drosophila melanogaster,Drosophila melanogaster: genetics,Drosophila melanogaster: metabolism,Embryo, Nonmammalian,Embryo, Nonmammalian: embryology,Embryo, Nonmammalian: metabolism,Female,Gene Dosage,Gene Dosage: genetics,Gene Expression Regulation, Developmental,Male,Models, Biological,Phenotype,Ploidies,RNA-Binding Proteins,RNA-Binding Proteins: genetics,RNA-Binding Proteins: metabolism,Sex Chromosomes,Sex Chromosomes: genetics,Sex Determination Processes,Time Factors,Transcription, Genetic,Transcription, Genetic: genetics},
month = {dec},
number = {12},
pages = {e332},
