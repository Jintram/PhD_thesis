journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Adaptation,Bacteria,Bacteria: metabolism,Bacterial Proteins,Bacterial Proteins: metabolism,Biological,Biological: physiology,Models,Proteome,Proteome: metabolism,Proteomics,Proteomics: methods,model},
