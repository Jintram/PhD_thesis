@article{VanDijk2015,
abstract = {The growth of single isogenic cells in the same environment shows a large degree of variability with consequences for resistance to antibiotics, chemotherapy, and environmental stress. In order to characterize the transcriptional differences associated with this variability, we developed a method â FitFlow â that enables physical sorting of subpopulations based on growth rate differences. We find that slow growing subpopulations in yeast have a substantially altered transcriptome that, to a limited extent, resembles that of cells growing slowly in a nutrient-limited chemostat. The cells of the slow growing subpopulation show a transcriptional stress response and have a high level of transcriptional diversity: they express more genes, novel transcripts, antisense transcripts, and genes on the 2-micron plasmid and from two endogenous viruses. Moreover, the slow subpopulation up-regulates transposons, and has reduced RNA polymerase fidelity. Finally, the slow cells show a transcriptional and post-translational DNA damage response, and addition of an antioxidant reduces the size of the slow subpopulation. Our results therefore suggest that the slow growing subpopulation explores a larger genotypic -and so phenotypic -space.},
author = {{Van Dijk}, David and Dhar, Riddhiman and Missarova, Alsu and Espinar, Lorena and Blevins, Will and Lehner, Ben and Carey, Lucas},
doi = {10.1038/ncomms8972},
file = {:C$\backslash$:/Users/wehrens/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Van Dijk et al. - 2015 - Slow growing subpopulations have increased DNA damage and RNA polymerase error rates.pdf:pdf},
